{
  "name": "dcabrio/dataframe",
  "description": "Archon: PHP Data Analysis Library",
  "keywords": ["data", "data analysis", "sql", "csv", "database", "excel"],
  "homepage": "https://github.com/HWGehring/Archon",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "ext-pdo_sqlite": "*",
    "gajus/dindent": "2.0.2",
    "phpoffice/phpspreadsheet": "^1.25"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phpmd/phpmd" : "@stable",
    "squizlabs/php_codesniffer": "2.3.4",
    "satooshi/php-coveralls": "0.6.1",
    "rector/rector": "^0.14.5"
  },
  "autoload": {
    "psr-4": { "Archon\\": "src/"}
  },
  "autoload-dev" : {
    "psr-4": { "Archon\\Tests\\": "tests/" }
  }
}
